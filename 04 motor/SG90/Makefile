CC=gcc
INC1 = 
INC2 = 
INCDIRS = -I${INC1} -I${INC2}
LIBDIR1 = 
LIBDIR2 = 
#CFLAGS=-g -Wall ${INCDIRS}	
CFLAGS=-g  ${INCDIRS}	
#LDFLAGS = -L${LIBDIR1} -L${LIBDIR2} -lwiringPi -lm -lrt
LDFLAGS = -lwiringPi 
LDFLAGS2 = -lpigpio -lrt -lpthread

SOURCES=sg90.c
OBJECTS=$(SOURCES:.c=.o)

SOURCES1=sg90_1.c
OBJECTS1=$(SOURCES1:.c=.o)

EXECUTABLE=sg90
EXECUTABLE1=sg90_1

all:$(EXECUTABLE) $(EXECUTABLE1) $(EXECUTABLE2)
$(EXECUTABLE):$(OBJECTS) 
	$(CC) $(CFLAGS) $(OBJECTS) $(LDFLAGS) -o $@   
$(EXECUTABLE1):$(OBJECTS1) 
	$(CC) $(CFLAGS) $(OBJECTS1) $(LDFLAGS2) -o $@   
clean:
	rm -f $(OBJECTS) $(OBJECTS1) $(OBJECTS2) $(EXECUTABLE) $(EXECUTABLE1)
