CC=gcc
INC1 = 
INC2 = 
INCDIRS = -I${INC1} -I${INC2}
LIBDIR1 = 
LIBDIR2 = 
CFLAGS=-g  ${INCDIRS}	
LDFLAGS = -lwiringPi -lm
SOURCES=pca9685_test.c
OBJECTS=$(SOURCES:.c=.o)

SOURCES1=pca9685_multi.c
OBJECTS1=$(SOURCES1:.c=.o)


EXECUTABLE=pca9685_test
EXECUTABLE1=pca9685_multi

all:$(EXECUTABLE) $(EXECUTABLE1) $(EXECUTABLE2)
$(EXECUTABLE):$(OBJECTS) 
	$(CC) $(CFLAGS) $(OBJECTS) $(LDFLAGS) -o $@   

$(EXECUTABLE1):$(OBJECTS1) 
	$(CC) $(CFLAGS) $(OBJECTS1) $(LDFLAGS) -o $@   


clean:
	rm -f $(OBJECTS) $(EXECUTABLE) $(OBJECTS1) $(EXECUTABLE1)

