CC=gcc
INC1 = 
INC2 = 
INCDIRS = -I${INC1} -I${INC2}
LIBDIR1 = 
LIBDIR2 = 
#CFLAGS=-g -Wall ${INCDIRS}	
CFLAGS=-g  ${INCDIRS}	
#LDFLAGS = -L${LIBDIR1} -L${LIBDIR2} -lwiringPi -lm
LDFLAGS = -lwiringPi -lm
SOURCES=mpu_6050.c
OBJECTS=$(SOURCES:.c=.o)

SOURCES1=mpu_6050_filter1.c
OBJECTS1=$(SOURCES1:.c=.o)

SOURCES2=mpu_6050_filter2.c
OBJECTS2=$(SOURCES2:.c=.o)

EXECUTABLE=mpu_6050
EXECUTABLE1=mpu_6050_filter1
EXECUTABLE2=mpu_6050_filter2

all:$(EXECUTABLE) $(EXECUTABLE1) $(EXECUTABLE2)
$(EXECUTABLE):$(OBJECTS) 
	$(CC) $(CFLAGS) $(OBJECTS) $(LDFLAGS) -o $@   

$(EXECUTABLE1):$(OBJECTS1) 
	$(CC) $(CFLAGS) $(OBJECTS1) $(LDFLAGS) -o $@   

$(EXECUTABLE2):$(OBJECTS2) 
	$(CC) $(CFLAGS) $(OBJECTS2) $(LDFLAGS) -o $@   

clean:
	rm -f $(OBJECTS) $(EXECUTABLE) $(OBJECTS1) $(EXECUTABLE1) $(OBJECTS2) $(EXECUTABLE2)

